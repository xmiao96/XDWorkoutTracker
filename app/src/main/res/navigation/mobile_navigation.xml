<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_process">

    <fragment
        android:id="@+id/navigation_process"
        android:name="com.example.xdworkouttracker.ui.process.ProcessFragment"
        android:label="@string/title_process"
        tools:layout="@layout/fragment_process" />

    <fragment
        android:id="@+id/navigation_alert"
        android:name="com.example.xdworkouttracker.ui.alert.AlertFragment"
        android:label="@string/title_alert"
        tools:layout="@layout/fragment_alert" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.xdworkouttracker.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.xdworkouttracker.HomeFragement.HomeFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_home" />

</navigation>